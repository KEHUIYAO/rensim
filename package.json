{
  "name": "puyosim",
  "version": "0.0.1",
  "scripts": {
    "android:start": "concurrently \"node node_modules/react-native/local-cli/cli.js start --reset-cache\" \"react-native run-android --no-packager --appIdSuffix debug\"",
    "android:build": "cd android && ./gradlew app:assembleRelease && ./gradlew installRelease",
    "android:emulator": "cd $ANDROID_HOME/emulator && ./emulator -avd \"Nexus_5X_API_27_x86\"",
    "ios:start": "concurrently \"node node_modules/react-native/local-cli/cli.js start --reset-cache --port 8082\" \"react-native run-ios --no-packager --port 8082\"",
    "web:start": "NODE_ENV=development webpack-dev-server --mode development --config web/webpack.config.js --watch",
    "web:build": "NODE_ENV=production webpack --config web/webpack.config.js --mode production",
    "web:deploy": "npm run web:build && firebase deploy --project puyosim-web",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "has-symbols": "^1.0.0",
    "i18n-js": "^3.1.0",
    "immer": "^1.9.3",
    "lodash": "^4.17.11",
    "query-string": "^6.2.0",
    "raven-for-redux": "^1.4.0",
    "react": "^16.7.0",
    "react-art": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hotkeys": "^1.1.4",
    "react-mixin": "^5.0.0",
    "react-modal": "^3.8.1",
    "react-native": "^0.57.8",
    "react-native-firebase": "5.1.1",
    "react-native-languages": "^3.0.1",
    "react-native-modal-selector": "1.0.0",
    "react-native-navigation": "^2.4.0",
    "react-native-sentry": "^0.40.2",
    "react-native-settings-list": "^1.8.0",
    "react-native-share": "^1.1.3",
    "react-native-splash-screen": "^3.1.1",
    "react-native-svg": "^8.0.10",
    "react-native-svg-web": "^1.0.1",
    "react-native-vector-icons": "^6.1.0",
    "react-native-version-number": "^0.3.5",
    "react-native-view-shot": "^2.5.0",
    "react-native-web": "^0.9.10",
    "react-native-web-responsive": "^1.0.5",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "realm": "^2.21.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.2",
    "reselect": "^4.0.0",
    "uuid": "^3.3.2",
    "webpack": "^4.28.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.1.4",
    "@storybook/addon-links": "^4.1.4",
    "@storybook/addons": "^4.1.4",
    "@storybook/react": "^4.1.4",
    "@types/jest": "^23.3.10",
    "@types/lodash": "^4.14.119",
    "@types/react": "^16.7.18",
    "@types/react-native": "^0.57.23",
    "@types/react-native-navigation": "^2.0.0",
    "@types/react-redux": "^6.0.11",
    "@types/react-router": "^4.4.3",
    "@types/realm": "^1.13.0",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.6",
    "@types/redux-saga": "^0.10.5",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__react": "^4.0.0",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-jest": "23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-plugin-react-native-web": "^0.9.10",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-native": "^4",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "concurrently": "^4.1.0",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "^0.51.1",
    "react-native-cli": "^2.0.1",
    "react-native-dotenv": "^0.2.0",
    "react-native-typescript-transformer": "^1.2.11",
    "react-test-renderer": "16.7.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.2",
    "typescript": "^3.2.2",
    "url-loader": "^1.1.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ]
  }
}
