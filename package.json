{
  "name": "puyosim",
  "version": "0.0.1",
  "scripts": {
    "android:start": "concurrently \"node node_modules/react-native/local-cli/cli.js start --reset-cache\" \"react-native run-android --no-packager --appIdSuffix debug\"",
    "android:build": "cd android && ./gradlew app:assembleRelease && ./gradlew installRelease",
    "android:emulator": "cd $ANDROID_HOME/tools && ./emulator -avd \"Nexus_5X_API_27_x86\"",
    "web:start": "NODE_ENV=development webpack-dev-server --mode development --config web/webpack.config.js --watch",
    "web:build": "NODE_ENV=production webpack --config web/webpack.config.js --mode production",
    "web:deploy": "npm run web:build && firebase deploy --project puyosim-web",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "has-symbols": "^1.0.0",
    "i18n-js": "^3.0.10",
    "immer": "^1.3.1",
    "lodash": "^4.17.10",
    "query-string": "^6.1.0",
    "raven-for-redux": "^1.3.1",
    "react": "16.4.1",
    "react-art": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hotkeys": "^1.1.4",
    "react-mixin": "^4.0.0",
    "react-modal": "^3.5.1",
    "react-native": "0.55.4",
    "react-native-firebase": "^4.3.8",
    "react-native-languages": "^3.0.1",
    "react-native-modal-selector": "0.0.27",
    "react-native-navigation": "^2.0.2580",
    "react-native-sentry": "^0.38.3",
    "react-native-settings-list": "^1.8.0",
    "react-native-share": "^1.1.2",
    "react-native-splash-screen": "^3.1.1",
    "react-native-svg": "^6.5.2",
    "react-native-svg-web": "^1.0.1",
    "react-native-vector-icons": "^4.6.0",
    "react-native-version-number": "^0.3.4",
    "react-native-view-shot": "^2.4.0",
    "react-native-web": "^0.8.8",
    "react-native-web-responsive": "^1.0.5",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "realm": "^2.12.0",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "webpack": "^4.14.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.0-alpha.20",
    "@storybook/addon-links": "^4.0.0-alpha.16",
    "@storybook/addons": "^3.4.10",
    "@storybook/react": "^4.0.0-alpha.16",
    "@types/jest": "^23.1.4",
    "@types/lodash": "^4.14.110",
    "@types/react": "^16.4.6",
    "@types/react-native": "^0.55.28",
    "@types/react-native-navigation": "^1.1.12",
    "@types/react-redux": "^6.0.3",
    "@types/react-router": "^4.0.31",
    "@types/realm": "^1.13.0",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.6",
    "@types/redux-saga": "^0.10.5",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__react": "^3.0.9",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-jest": "23.4.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-plugin-react-native-web": "^0.8.8",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-native": "^4",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "concurrently": "^3.6.0",
    "jest": "23.4.2",
    "react-native-cli": "^2.0.1",
    "react-native-dotenv": "^0.2.0",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.4.1",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.4.2",
    "typescript": "^2.9.2",
    "url-loader": "^1.0.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.5"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ]
  }
}
